<views:MvxWpfView x:TypeArguments="viewModels:MainViewModel" x:Class="RestaurantAccounting.WPF.Pages.MainPage"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:local="clr-namespace:RestaurantAccounting.WPF.Pages"
                  xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
                  xmlns:viewModels="clr-namespace:RestaurantAccounting.Core.ViewModels;assembly=RestaurantAccounting.Core"
                  mc:Ignorable="d"
                  d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="WELCOME" FontWeight="Bold" FontSize="25"/>
            <Button Content="Open profile" Command="{Binding OpenProfileCommand}"/>
        </StackPanel>
    </Grid>
</views:MvxWpfView>
